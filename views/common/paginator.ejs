<div class="paginator pager clearfix">
        <% if(paginator.pageNo>2) { %>
        <a href="/page/<%= (paginator.pageNo-1) %>" class="paging pre">
            <div class="arrow icon-arrow2_left"></div>
        <span>上一页
        </span></a>
        <% } else if (paginator.pageNo == 2 ) { %>
            <a href="/" class="paging pre">
                <div class="arrow icon-arrow2_left"></div>
            <span>上一页
            </span></a>
        <% } %>
        <% if((paginator.pageNo)<=(paginator.totalCount/paginator.pageSize) ) { %>
        <a href="/page/<%= (paginator.pageNo+1) %>" class="paging next">
            <div class="arrow icon-arrow2_right"></div>
        <span>下一页
        </span></a>
        <% }%>
        <div class="clear"></div>
</div>